<!DOCTYPE html>
<html>
    <head>
    <% include ../../partials/header.ejs %>
    <link href="../../plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
    </head>
<body class="theme-blue-grey">
    <% include ../../partials/page_loader.ejs %>
    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>
    <!--Search bar-->
    <% include ../../partials/search_bar.ejs %>
   
    <!-- Top Bar -->
    <% include ../../partials/navigation.ejs %>
    
    <!-- Left side bar -->
    <% include ../../partials/left_sidebar.ejs %>

    <section class="content">
        <div class="container-fluid">
            <div class="block-header">
                <h2><DASHBOARD></h2>
            </div>

            <!-- Widgets -->
            <div class="row clearfix">
                

                <% topPanel.forEach(function(topWidget) { %>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <div class="info-box <%=topWidget.color %> hover-expand-effect">
                            <div class="icon">
                                <i class="material-icons"><%=topWidget.icon %></i>
                            </div>
                            <div class="content">
                                <div class="text"><%=topWidget.name %></div>
                                <div class="number"><%=topWidget.value %></div>
                            </div>
                        </div>
                    </div>
                <%}); %>
            </div>
            <!-- #END# Widgets -->
            
            <!-- Tab panel -->
            <div class="row clearfix">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <% include ./component/tabs.ejs %>
                </div>
            </div>
            <!--End Tab panel -->
        </div>
    </section>

    <% include ../../partials/scripts.ejs %>

</body>

</html>

