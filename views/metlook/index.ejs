<!DOCTYPE html>
<html>
    <head>
    <% include partials/header.ejs %>
    </head>
<body class="theme-blue-grey">
    <% include partials/page_loader.ejs %>
    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>
   <!-- Top Bar -->
    <nav class="navbar">
        <div class="container-fluid">
            <div class="navbar-header">
              <a href="javascript:void(0);" class="bars"></a>
              <a class="navbar-brand" href="#">PIXSELL - W  <%= baseFrame.title %></a>
            </div>
        </div>
    </nav> 
    <!-- Left side bar -->
    <section>
        <!-- Left Sidebar -->
        <aside id="leftsidebar" class="sidebar">
        <!-- User Info -->
            <div class="user-info"> 
               <div class="info-container">
            </div>
            </div>
                <!-- #User Info -->
                <!-- Menu -->
                <div class="menu">
                    <ul class="list">
                        <li class="header">Welcome to PIXSELL</li>
        
                        <% sideMenu.forEach(function(site) { %>
                            <li class=<%=site.state %> >
                                <a href=<%=site.link %> >
                                    <i class="material-icons"><%=site.icon %></i>
                                    <span><%=site.name %></span>
                                </a>
                            </li>
                        <%}); %>
                    </ul>
                </div>
                <!-- #Menu -->
                <!-- Footer -->
                <div class="legal">
                    <div class="copyright">
                        &copy; 2017 <a href="javascript:void(0);">PIXSELL - Legator Solutions</a>.
                    </div>
                    <div class="version">
                        <b>Version: </b> 0.1
                    </div>
                </div>
                <!-- #Footer -->
            </aside>
            <!-- #END# Left Sidebar -->
        </section>
        

    <section class="content">
        <div class="container-fluid">
            <div class="block-header">
            </div>

            <!-- Widgets -->
            <div class="row clearfix">
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box bg-pink hover-expand-effect">
                        <div class="icon">
                            <i class="material-icons">dashboard</i>
                        </div>
                        <div class="content">
                            <div class="text">Active AD-Blocks</div>
                            <div class="number count-to" data-from="0" data-to="145" data-speed="1000" data-fresh-interval="20"></div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box bg-cyan hover-expand-effect">
                        <div class="icon">
                            <i class="material-icons">web</i>
                        </div>
                        <div class="content">
                            <div class="text">Web Sites</div>
                            <div class="number count-to" data-from="0" data-to="54" data-speed="1000" data-fresh-interval="20"></div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box bg-light-green hover-expand-effect">
                        <div class="icon">
                            <i class="material-icons">monetization_on</i>
                        </div>
                        <div class="content">
                            <div class="text">Micro investors</div>
                            <div class="number count-to" data-from="0" data-to="2483" data-speed="800" data-fresh-interval="20"></div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box bg-orange hover-expand-effect">
                        <div class="icon">
                            <i class="material-icons">person_add</i>
                        </div>
                        <div class="content">
                            <div class="text">NEW VISITORS</div>
                            <div class="number count-to" data-from="0" data-to="1225" data-speed="1000" data-fresh-interval="20"></div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- #END# Widgets -->
           
            <div class="row clearfix">
                <!-- Visitors -->
                <div class="col-xs-12 col-sm-12 col-md-4 col-lg-6">
                    <a style="text-decoration:none;" href="webSiteOwner">
                        <div class="card">
                            <div class="body bg-light-green">
                                <div class="m-b--35 font-bold"><center><h3>LogIn as WebSite     </h3></center></div>
                                <ul class="dashboard-stat-list">
                                    <li>
                                        #Ad-Blocks Registration
                                        <span class="pull-right">
                                            <i class="material-icons">dashboard</i>
                                        </span>
                                    </li>
                                    <li>
                                        #Open for Micro investment
                                        <span class="pull-right">
                                            <i class="material-icons">attach_money</i>
                                        </span>
                                    </li>
                                    <li>
                                        #Earn Money More and More
                                        <span class="pull-right">
                                            <i class="material-icons">trending_up</i>
                                        </span>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </a>
                </div>
                <!-- #END# Visitors -->
                <!-- Latest Social Trends -->
                <div class="col-xs-12 col-sm-12 col-md-4 col-lg-6">
                    <a style="text-decoration:none;" href="adClient">
                    <div class="card">
                        <div class="body bg-cyan">
                            <div class="m-b--35 font-bold"><center><h3>LogIn as Advertising Clients</h3></center></div>
                            <ul class="dashboard-stat-list">
                                <li>
                                    #Ad-campaigns
                                    <span class="pull-right">
                                        <i class="material-icons">event</i>
                                    </span>
                                </li>
                                <li>
                                    #Demand base Advertising
                                    <span class="pull-right">
                                        <i class="material-icons">flight_takeoff</i>
                                    </span>
                                </li>
                                <li>
                                    #Publicity More and More
                                    <span class="pull-right">
                                        <i class="material-icons">trending_up</i>
                                    </span>
                                </li>
                            </ul>
                        </div>
                    </div>
                    </a>
                </div>
                <!-- #END# Latest Social Trends -->
            </div>
        </div>
    </section>

    <% include partials/scripts.ejs %>
</body>

</html>

